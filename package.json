{
  "name": "bahn.expert",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "engines": {
    "node": "^20.0.0"
  },
  "private": true,
  "engineStrict": true,
  "packageManager": "pnpm@8.6.2",
  "jest-junit": {
    "output": "./reports/junitresults.xml"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "footer-leading-blank": [
        0
      ],
      "subject-case": [
        0
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80
  },
  "percy": {
    "version": 2,
    "snapshot": {
      "widths": [
        375,
        1366
      ]
    }
  },
  "browserslist": [
    ">2% in DE",
    "not ie 9",
    "firefox 68"
  ],
  "scripts": {
    "build:clean": "rimraf dist && mkdir dist && rimraf -g 'src/**/**/*.js'",
    "build:client": "node scripts/buildClient.cjs",
    "build:server": "node scripts/buildServer.cjs",
    "build": "run-p build:client build:server",
    "dev": "node devIndex.cjs",
    "lint": "run-p lint:eslint lint:prettier",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx scripts cypress src",
    "lint:prettier": "prettier -c src scripts cypress",
    "test": "run-p lint test:jest test:ts",
    "test:jest": "jest",
    "test:ts": "run-p test:ts:main test:ts:cypress",
    "test:ts:main": "tsc",
    "test:ts:cypress": "tsc -p cypress",
    "e2e:local": "TZ=Etc/UTC cypress open",
    "e2e:local:run": "TZ=Etc/UTC percy exec -- cypress run",
    "doc:start": "./startDocServer.sh",
    "doc:build": "tsoa spec-and-routes && node ./scripts/adjustSpecJson.cjs",
    "doc:build:gitadd": "pnpm doc:build && git add public/swagger.json && git add src/server/API/routes.ts",
    "lint-staged": "lint-staged",
    "postinstall": "husky install",
    "precommit": "run-s doc:build:gitadd lint-staged",
    "generate:risStations": "openapi-generator-cli generate --additional-properties=useSingleRequestParameter=true -i ./src/business-hub/ris-stations-1.10.0.yaml -o src/business-hub/generated/risStations -g typescript-axios"
  },
  "lint-staged": {
    "*.{t,j,cj,mj}s?(x)": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.json": [
      "prettier --write"
    ]
  },
