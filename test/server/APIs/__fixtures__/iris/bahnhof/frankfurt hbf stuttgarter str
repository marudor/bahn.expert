sort: false,
data: function (i) {return {label: i.value, value: i.value, url: i.url}}
});
$("#2-973r1qi0dxt56m1k1v25dyaus").on("keyup", function (e) {
if (e.which <= 90 && e.which >= 48) {
$.ajax({url: "/service/search/bahnhof-de/520608?rootNavigationId=511938&query=" + encodeURIComponent($("#2-973r1qi0dxt56m1k1v25dyaus").val()) + "", type: 'GET', dataType: 'json'})
.success(function (d) {awe.list = d;})
}
}).on("awesomplete-selectcomplete", function (evt) {
if (evt.originalEvent.text) {
var selectedSuggestion = evt.originalEvent.text;
if (selectedSuggestion.url) {
window.location.href = selectedSuggestion.url;
} else {
$(this).closest("form").submit();
}
}
});
}();
if ($('.suche-kontext-filter').length == 0) {
$('div[role=search]').css('top', '-198px');
}
