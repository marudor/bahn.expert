{
  "name": "business-hub",
  "version": "1.0.0",
  "author": "marudor",
  "scripts": {
    "build": "run-s build:clean build:babel",
    "build:babel": "SERVER=1 babel -x .ts --root-mode upward . --out-dir . --ignore 'node_modules/**'",
    "build:clean": "rimraf '*[!node_modules]**/**/*.js' && rimraf '*.js'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "generate:apiClient": "openapi-generator-cli generate --additional-properties=useSingleRequestParameter=true -i ./ris-stations-1.7.0.yaml -o generated -g typescript-axios"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "types": "workspace:*"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@types/babel__core": "7.1.19",
    "@types/rimraf": "^3.0.2",
    "eslint": "^8.18.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "bundledDependencies": [
    "business-hub"
  ]
}
