{
  "entryFile": "./API/index.ts",
  "controllerPathGlobs": ["./API/controller/**", "../types/**"],
  "noImplicitAdditionalProperties": "silently-remove-extras",
  "spec": {
    "name": "Bahnhofs Abfahrten",
    "specVersion": 3,
    "outputDirectory": "../../public",
    "host": "marudor.de",
    "basePath": "api/",
    "tags": [
      {
        "name": "HAFAS",
        "description": "All Operation use a HAFAS backend. By default it will be the HAFAS backend by DB Vertrieb (DB Navigator). Different backends can be selected by providing the query paramter profile."
      },
      {
        "name": "HAFAS Subscribe"
      },
      {
        "name": "HAFAS Experimental"
      },
      {
        "name": "IRIS"
      },
      {
        "name": "OEBB"
      },
      {
        "name": "Reihung"
      },
      {
        "name": "StopPlace"
      }
    ]
  },
  "routes": {
    "middleware": "koa",
    "routesDir": "./API",
    "basePath": "api/"
  }
}
